            <div id='search-control-panel'>
                <div class="panel-group" role="tablist" aria-multiselectable="true">
                    <div class="panel panel-default">
                        <div data-toggle="collapse" href="#reddit-modal-collapse" class="panel-heading text-center" role="tab">
                            <h4 class="panel-title white"><i class='fa fa-search'></i>&nbsp;Search Reddit</h4>
                        </div>
                        <div id="reddit-modal-collapse" class="panel-collapse collapse" role="tabpanel" aria-expanded="true">
                            <div class="panel-body">
                                <form class='reddit-search' role='form' action='javascript:void(0)'>
                                    <div class="form-group">
                                        <label>Search</label>
                                        <input class="form-control reddit-search-input" type="text" placeholder='Enter query..'>
                                    </div>
                                    <div class="form-group">
                                        <label class="radio-inline">
                                            <input class='radio-subreddits' name='search-type' checked type="radio" value="subreddits" {{pressType}}="bindRedditSearchRadio.call(this)">Subreddits
                                        </label>
                                        <label class="radio-inline">
                                            <input class='radio-submissions' name='search-type' type="radio" value="submissions" {{pressType}}="bindRedditSearchRadio.call(this)">Submissions
                                        </label>  
                                    </div>
                                    <div class="form-group">
                                        <label>Sorted By</label>
                                        <select disabled class="form-control sorted-by">
                                            <option value='relevance'>Relevance</option>
                                            <option value='new'>New</option>
                                            <option value='hot'>Hot</option>
                                            <option value='top'>Top</option>
                                            <option value='comments'>Comments</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label>Links From</label>
                                        <select disabled class="form-control links-from">
                                            <option value='all'>All Time</option>
                                            <option value='hour'>Past Hour</option>
                                            <option value='day'>Past 24 Hours</option>
                                            <option value='week'>Past Week</option>
                                            <option value='year'>Past Year</option>    
                                        </select>
                                    </div>
                                    <button type="submit" class="reddit-search-submit btn btn-default" {{pressType}}="bindRedditSearch.call(this, hideControlModal)">Submit</button>
                                    <input class='reddit-search-submit hide' type='submit' {{pressType}}="bindRedditSearch.call(this, hideControlModal)">                          
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-group" role="tablist" aria-multiselectable="true">
                    <div class="panel panel-default">
                        <div data-toggle="collapse" href="#twitter-modal-collapse" class="panel-heading text-center" role="tab">
                            <h4 class="panel-title"><i class='fa fa-search'></i>&nbsp;Search Twitter</h4>
                        </div>
                        <div id="twitter-modal-collapse" class="panel-collapse collapse" role="tabpanel" aria-expanded="true">
                        </div>
                    </div>
                </div>
            </div>
